<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="specFlow" type="TechTalk.SpecFlow.Configuration.ConfigurationSectionHandler, TechTalk.SpecFlow" />
  </configSections>

  <appSettings>
    <add key="SiteUrl" value="http://localhost:58355/" />
    <add key="RegisterUrl" value="/Account/Register" />
    <add key="LoginUrl" value="/Account/Login"/>
    <add key="FolderPicture" value="\ScreenShots\"/>
    <add key="ChromeDrive" value="A:\Selenium\ChromeDriverOld"/>

    <add key="BrowserType" value="1" />
  </appSettings>
  
  <specFlow>
    <language feature="pt-BR" />
    <!-- For additional details on SpecFlow configuration options see http://go.specflow.org/doc-config -->
    <unitTestProvider name="xUnit" />
  </specFlow>
</configuration>